            <rtexprvalue>true</rtexprvalue>
            <description><![CDATA[The length should the text display]]></description>
        </attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>Support the STYLE attribute</description>
        </attribute>
        <attribute>
            <name>more</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description><![CDATA[The tail content after the text body]]></description>
        </attribute>
        <attribute>
            <name>showMore</name>
            <required>false</required>
