<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>

    <tlib-version>2.2</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>pengpeng Taglib</short-name>
    <uri>/pengpeng</uri>
    <display-name>pengpeng Taglib</display-name>
    <description><![CDATA[pengpeng JSP Taglib]]></description>

    <tag>

        <name>translate</name>
        <tag-class>com.pengpeng.common.ui.tags.TranslateConstantsTag</tag-class>
        <body-content>empty</body-content>
        <description><![CDATA[Translate a code into a title value from the OptionHelper]]></description>

        <attribute>
            <name>code</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description><![CDATA[The Code value to the Title]]></description>

        </attribute>

        <attribute>
            <name>typeCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description>
                <![CDATA[A type value defined in the com.pengpeng.common.constants.OptionConstants]]></description>

        </attribute>

        <attribute>
            <name>subTypeCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description>
                <![CDATA[When using a typeCode as OptionConstants.CITY, this field should be set to get the Title correctly]]></description>

        </attribute>

        <attribute>
            <name>wwCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description><![CDATA[A code property name defined in xwork value stack]]></description>

        </attribute>

        <attribute>
            <name>wwTypeCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description><![CDATA[A type property name defined in xwork value stack]]></description>

        </attribute>

        <attribute>
            <name>wwSubTypeCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description><![CDATA[A sub type property name defined in xwork value stack]]></description>

        </attribute>

        <attribute>
            <name>cssClass</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description><![CDATA[The css class to use]]></description>

        </attribute>

        <attribute>
            <name>style</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description><![CDATA[The css style to use]]></description>

        </attribute>

        <attribute>
            <name>defaultValue</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description><![CDATA[The default value]]></description>

        </attribute>

        <attribute>
            <name>isHtml</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>

            <description><![CDATA[The isHtml value]]></description>

        </attribute>
    </tag>


    <tag>

