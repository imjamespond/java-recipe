    <tag>
        <name>text</name>
        <tag-class>com.pengpeng.common.ui.tags.TextTag</tag-class>
        <body-content>JSP</body-content>
        <description><![CDATA[Text tag help you to manage your output text]]></description>

        <attribute>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description><![CDATA[The html ID element of the text span]]></description>
        </attribute>
        <attribute>
            <name>clearHTML</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description><![CDATA[The html ID element of the text span]]></description>
        </attribute>
        <attribute>
            <name>displayLength</name>
