openssl genrsa -des3 -out server.key 1024
openssl rsa -in server.key -pubout -out public.key
openssl rsautl -encrypt -in hello -inkey public.key -pubin -out pub.encrypy.hello
openssl rsautl -decrypt -in pub.encrypy.hello -inkey server.key -out pub.decrypy.hello
openssl req -new -key server.key -out cert.req.csr
openssl x509 -req -in cert.req.csr -out cert.pem -signkey server.key -days 365


